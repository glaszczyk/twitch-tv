{
	"extends": [
		"airbnb"
	],
	"plugins": [
		"react",
		"jsx-a11y",
		"import"
	],
	"rules": {
		"jsx-a11y/img-has-alt": 1,
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		],
		"indent": [
			1,
			"tab",
			{
				"SwitchCase": 1,
				"VariableDeclarator": 1
			}
		],
		"no-tabs": 0,
		"react/prop-types": 0,
		"react/jsx-indent": [
			1,
			"tab"
		],
		"react/jsx-indent-props": [
			1,
			"tab"
		]
	}
}
